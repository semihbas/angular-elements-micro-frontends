          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/client-documents/tsconfig.app.json",
              "projects/client-documents/tsconfig.spec.json",
              "projects/client-documents/e2e/tsconfig.json"
            ],
            "exclude": [
