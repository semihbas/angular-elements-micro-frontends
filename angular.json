{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "client-reports": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/client-reports",
      "sourceRoot": "projects/client-reports/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/client-reports",
            "index": "projects/client-reports/src/index.html",
            "main": "projects/client-reports/src/main.ts",
            "polyfills": "projects/client-reports/src/polyfills.ts",
            "tsConfig": "projects/client-reports/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/client-reports/src/favicon.ico",
              "projects/client-reports/src/assets"
            ],
            "styles": [
              "projects/client-reports/src/styles.css",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": [{
                "bundleName": "polyfill-webcomp-es5",
                "input": "node_modules/@webcomponents/webcomponentsjs/custom-elements-es5-adapter.js"
              },
              {
                "bundleName": "polyfill-webcomp",
                "input": "node_modules/@webcomponents/webcomponentsjs/bundles/webcomponents-sd-ce-pf.js"
              },
              "node_modules/rxjs/bundles/rxjs.umd.js",
              "node_modules/@angular/core/bundles/core.umd.js",
              "node_modules/@angular/common/bundles/common.umd.js",
              "node_modules/@angular/common/bundles/common-http.umd.js",
              "node_modules/@angular/compiler/bundles/compiler.umd.js",
              "node_modules/@angular/elements/bundles/elements.umd.js",
              "node_modules/@angular/router/bundles/router.umd.js",
              "node_modules/@angular/platform-browser/bundles/platform-browser.umd.js",
              "node_modules/@angular/platform-browser-dynamic/bundles/platform-browser-dynamic.umd.js",



              "node_modules/@angular/cdk/bundles/cdk.umd.js",
              "node_modules/@angular/cdk/bundles/cdk-keycodes.umd.js",
              "node_modules/@angular/cdk/bundles/cdk-coercion.umd.js",
              "node_modules/@angular/cdk/bundles/cdk-observers.umd.js",
              "node_modules/@angular/cdk/bundles/cdk-platform.umd.js",
              "node_modules/@angular/cdk/bundles/cdk-bidi.umd.js",
              "node_modules/@angular/cdk/bundles/cdk-a11y.umd.js",
              "node_modules/@angular/cdk/bundles/cdk-portal.umd.js",
              "node_modules/@angular/animations/bundles/animations.umd.js",
              "node_modules/@angular/animations/bundles/animations-browser.umd.js",
              "node_modules/@angular/platform-browser/bundles/platform-browser-animations.umd.js",

              "node_modules/@angular/material/bundles/material.umd.js",
              "node_modules/@angular/material/bundles/material-core.umd.js",
              "node_modules/@angular/material/bundles/material-button.umd.js",
              "node_modules/@angular/material/bundles/material-card.umd.js",
              "node_modules/@angular/material/bundles/material-datepicker.umd.js",
              "node_modules/@angular/material/bundles/material-form-field.umd.js",
              "node_modules/@angular/material/bundles/material-icon.umd.js",
              "node_modules/@angular/material/bundles/material-input.umd.js",
              "node_modules/@angular/material/bundles/material-list.umd.js",
              "node_modules/@angular/material/bundles/material-paginator.umd.js",
              "node_modules/@angular/material/bundles/material-sort.umd.js",
              "node_modules/@angular/material/bundles/material-table.umd.js",
              "node_modules/@angular/material/bundles/material-tooltip.umd.js"



            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "projects/client-reports/src/environments/environment.ts",
                "with": "projects/client-reports/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "60kb",
                  "maximumError": "100kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "client-reports:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-reports:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client-reports:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "projects/client-reports/src/test.ts",
            "polyfills": "projects/client-reports/src/polyfills.ts",
            "tsConfig": "projects/client-reports/tsconfig.spec.json",
            "karmaConfig": "projects/client-reports/karma.conf.js",
            "assets": [
              "projects/client-reports/src/favicon.ico",
              "projects/client-reports/src/assets"
            ],
            "styles": [
              "projects/client-reports/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/client-reports/tsconfig.app.json",
              "projects/client-reports/tsconfig.spec.json",
              "projects/client-reports/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/client-reports/e2e/protractor.conf.js",
            "devServerTarget": "client-reports:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "client-reports:serve:production"
            }
          }
        }
      }
    },
    "client-documents": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/client-documents",
      "sourceRoot": "projects/client-documents/src",
      "prefix": "app",
